{
  "name": "frontmatter-markdown-loader",
  "version": "2.1.0",
  "description": "Webpack loader for Front Matter-ed Markdown file to get frontmatter attributes and compiled Vue component",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./docs/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hmsk/frontmatter-markdown-loader.git"
  },
  "keywords": [
    "webpack",
    "markdown",
    "frontmatter",
    "vue",
    "loader"
  ],
  "author": "Kengo Hamasaki <k.hamasaki@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmsk/frontmatter-markdown-loader/issues"
  },
  "homepage": "https://github.com/hmsk/frontmatter-markdown-loader#readme",
  "dependencies": {
    "front-matter": "^3.0.1",
    "loader-utils": "^1.2.3",
    "markdown-it": "^10.0.0"
  },
  "optionalDependencies": {
    "vue-template-compiler": "^2.5.0",
    "vue-template-es2015-compiler": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.3",
    "@babel/preset-env": "7.6.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "jest": "24.9.0",
    "node-eval": "2.0.0",
    "vue": "2.6.10",
    "vue-jest": "4.0.0-beta.2",
    "vue-template-compiler": "2.6.10",
    "vue-template-es2015-compiler": "1.9.1",
    "vuepress": "1.1.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  }
}
